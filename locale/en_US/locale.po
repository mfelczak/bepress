msgid "plugins.importexport.bepress.displayName"
msgstr "Bepress Import Plugin"

msgid "plugins.importexport.bepress.description"
msgstr "Import articles into journal from Bepress XML."

msgid "plugins.importexport.bepress.cliUsage"
msgstr ""
"Command-line tool for importing Bepress XML article data into OJS\n"
"\t\t\tUsage:\n"
"\t\t\t{$scriptName} journal username editorUsername defaultEmail importPath\n"
"\t\t\tjournal - the journal into which the articles should be imported (use journal path)\n"
"\t\t\tusername - the user to whom imported articles should be assigned; note: this user must have the Author role\n"
"\t\t\teditorUsername - the editor to whom imported articles should be assigned; note: this user must have the Journal Editor role and access to the Production stage\n"
"\t\t\tdefaultEmail - assigned to article metadata when author email not provided in import XML\n"
"\t\t\timportPath - full filepath to import XML/PDF files (e.g. /home/user/import_files); folder structure must be of the form vol#/iss#/article#\n"
""

